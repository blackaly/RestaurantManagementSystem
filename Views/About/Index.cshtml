                            <form class="clearfix">
                                <div class="select-sub">
                                    <span>CHECK IN</span>
                                    <div class="icon"><img src="img/bg/ap-icon01.png" alt="ap-icon"></div>
                                    <div class="form-group">
                                        <div id="filterDate">
                                            <!-- Datepicker as text field -->
                                            <div class="input-group date" data-date-format="dd.mm.yyyy">
                                                <input type="text" class="form-control" placeholder="14.07.2020" required="">
                                                <div class="input-group-addon">
                                                    <span class="ti-angle-down"></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="select-sub">
                                    <span>GUESTS</span>
                                    <div class="icon"><img src="img/bg/ap-icon02.png" alt="ap-icon"></div>
                                    <select class="select wide">
                                        <option>02</option>
                                        <option>01</option>
                                        <option>03</option>
                                        <option>04</option>
                                        <option>05</option>
                                        <option>06</option>
                                    </select>
                                </div>

                                <div class="select-sub">
                                    <button class="btn ss-btn" type="submit">book now</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
